<?xml version="1.0" encoding="UTF-8"?>
<Definitions id="definitions" name="camunda" namespace="http://camunda.org/dmn" xmlns="http://www.omg.org/spec/DMN/20130901" xmlns:camunda="http://camunda.org/schema/1.0/dmn">
  <ItemDefinition id="itemDefinition2" name="ProductNameType">
    <typeDefinition>string</typeDefinition>
    <allowedValue id="allowedValue1">
      <text>BlueCar</text>
    </allowedValue>
    <allowedValue id="allowedValue2">
      <text>RedCar</text>
    </allowedValue>
  </ItemDefinition>
  <ItemDefinition id="itemDefinition3" name="CheckResultType">
    <typeDefinition>string</typeDefinition>
    <allowedValue id="allowedValue4">
      <text>ok</text>
    </allowedValue>
    <allowedValue id="allowedValue5">
      <text>notok</text>
    </allowedValue>
  </ItemDefinition>
  <Decision id="decision" name="State2Ship">
    <DecisionTable id="decisionTable" isComplete="true" isConsistent="true" name="State2Ship">
      <clause id="clause2" name="ProductName">
        <inputExpression id="inputExpression2" name="Product">
          <itemDefinition href="#itemDefinition2"/>
          <text>product</text>
        </inputExpression>
        <inputEntry id="inputEntry4">
          <text>RedCar</text>
        </inputEntry>
        <inputEntry id="inputEntry5">
          <text>BlueCar</text>
        </inputEntry>
      </clause>
      <clause camunda:name="result" id="clause3" name="Check Result">
        <outputDefinition href="#itemDefinition3"/>
        <outputEntry id="outputEntry1">
          <text>notok</text>
        </outputEntry>
        <outputEntry id="outputEntry2">
          <text>ok</text>
        </outputEntry>
      </clause>
      <rule id="rule1">
        <condition>inputEntry4</condition>
        <conclusion>outputEntry1</conclusion>
      </rule>
    </DecisionTable>
  </Decision>
</Definitions>