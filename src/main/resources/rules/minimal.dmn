<?xml version="1.0" encoding="UTF-8"?>
<Definitions id="definitions"
             name="camunda"
             namespace="http://camunda.org/dmn"
             xmlns="http://www.omg.org/spec/DMN/20130901"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn">
  <ItemDefinition id="itemDefinition1" name="AdressStateType">
    <typeDefinition>string</typeDefinition>
  </ItemDefinition>
  <ItemDefinition id="itemDefinition3" name="CheckResultType">
    <typeDefinition>string</typeDefinition>
    <allowedValue id="allowedValue4">
      <text>ok</text>
    </allowedValue>
    <allowedValue id="allowedValue5">
      <text>notok</text>
    </allowedValue>
  </ItemDefinition>
  <Decision id="decision" name="State2Ship">
    <DecisionTable id="decisionTable" isComplete="true" isConsistent="true" name="State2Ship">
      <clause id="clause1" name="AdressState">
        <inputExpression id="inputExpression1" name="State">
          <itemDefinition href="#itemDefinition1"/>
          <text>state</text>
        </inputExpression>
        <inputEntry id="inputEntry1">
          <text>Afghanistan</text>
        </inputEntry>
        <inputEntry id="inputEntry2">
          <text>Germany</text>
        </inputEntry>
      </clause>
      <clause camunda:name="result" id="clause3" name="Check Result">
        <outputDefinition href="#itemDefinition3"/>
        <outputEntry id="outputEntry1">
          <text>notok</text>
        </outputEntry>
        <outputEntry id="outputEntry2">
          <text>ok</text>
        </outputEntry>
      </clause>
      <rule id="rule1">
        <condition>inputEntry1</condition>
        <conclusion>outputEntry1</conclusion>
      </rule>
    </DecisionTable>
  </Decision>
</Definitions>